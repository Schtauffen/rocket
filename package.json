{
  "name": "cycle-tinkering",
  "version": "1.0.0",
  "description": "webpack/cycle starter",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "styl": "stylus -u jeet src/styles/main.styl -o public/main.css",
    "min": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "closure": "webpack -p --config webpack.closure.config.js",
    "dev": "NODE_ENV=develop webpack -w & stylus -u jeet -w src/styles/main.styl -o public/main.css & nodemon server.js && fg",
    "start": "NODE_ENV=production nodemon server.js"
  },
  "author": "Zach Dahl <z.schtauffen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.4.0",
    "@cycle/http": "^8.2.2",
    "@cycle/isolate": "^1.2.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jeet": "^6.1.2",
    "js-csp": "^0.5.0",
    "nodemon": "^1.9.1",
    "rupture": "^0.6.1",
    "rx": "^4.1.0",
    "stylus": "^0.53.0",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.12.14",
    "webpack-closure-compiler": "^2.0.2"
  },
  "dependencies": {
    "koa": "^1.2.0",
    "koa-compress": "^1.0.9",
    "koa-router": "^5.4.0"
  }
}
